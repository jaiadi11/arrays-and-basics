class Solution(object):
    def maxProfit(self, prices):
        """
        :type prices: List[int]
        :rtype: int
        """
        minp=float('inf')
        profit=0
        for i in prices:
            if i <=minp:
                minp=i
            elif i-minp>profit:
                profit=i-minp
        return profit
                
            
        
    
            
        
        
